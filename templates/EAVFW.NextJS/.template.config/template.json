{
  "$schema": "http://json.schemastore.org/template",
  "author": "Thyge Steffensen",
  "classifications": [
    "Common",
    "Library"
  ],
  "tags": {
    "language": "C#",
    "type": "solution"
  },
  "identity": "EAVFW-NextJS",
  "name": "EAV Framework",
  "shortName": "eavfw-nextjs",
  "symbols": {
    "projectPrefix": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "EAVFW",
      "replaces": "__EAVFW__",
      "FileRename": "__EAVFW__",
      "isRequired": true
    },
    "mainAppReplace": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "MainApp",
      "replaces": "__MainApp__",
      "FileRename": "__MainApp__",
      "isRequired": true
    },
    "databaseName": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "databaseName",
      "replaces": "databaseName",
      "isRequired": true
    }
  },
  "sources": [
    {
      "source": "./",
      "target": "./",
      "exclude": [
        "**/[Bb]in/**",
        "**/[Oo]bj/**",
        ".template.config/**/*",
        "**/*.filelist",
        "**/*.user",
        "**/*.lock.json",
        "**/.vs/**",
        "**/.idea/**",
        "**/node_modules/**",
        "**/.git/**"
      ]
    }
  ],
  "postActions": [
   
  ]
}
{
  "name": "monorepo-typescript-next-the-sane-way",
  "private": true,
  "workspaces": [
    "apps/__EAVFW__.__MainApp__"
  ],
  "scripts": {
    "gm": "dotnet msbuild ./apps/__EAVFW__.__MainApp__/__EAVFW__.__MainApp__.csproj /target:GenerateManifest",
    "build-portal": "npm run build --prefix apps/__EAVFW__.__MainApp__",
    "buildforrun": "npm run build --prefix apps/__EAVFW__.__MainApp__",
    "localdev": "npm run start --prefix apps/__EAVFW__.__MainApp__",
    "linkEAV": "npm link @eavfw/apps @eavfw/next @eavfw/expressions @eavfw/manifest @eavfw/hooks @eavfw/forms @eavfw/utils",
    "eav-migrate-latest": "dotnet msbuild /target:GenerateManifest ./apps/__EAVFW__.__MainApp__ && dotnet test --filter \"ClassName=__EAVFW__.ArbejdstidsRegistrering.Scripts.DBDevSetup\" scripts/__EAVFW__.ArbejdstidsRegistrering.Scripts/__EAVFW__.ArbejdstidsRegistrering.Scripts.csproj && docker exec -i __EAVFW__ /opt/mssql-tools/bin/sqlcmd -s localhost -U sa -P Bigs3cRet -d __EAVFW__ -I -i /opt/dbinit/init.sql -i /opt/dbinit/init-systemadmin.sql -v DBName=__EAVFW__ -v DBSchema=__EAVFW__ -v SystemAdminSecurityGroupId=1b714972-8d0a-4feb-b166-08d93c6ae328 -v UserGuid=1b714972-8d0a-4feb-b166-08d93c6ae329 -v UserName=\"Poul Kjeldager\" -v UserEmail=pks@delegate.dk -v UserPrincipalName=PoulKjeldagerSÃ¸rense",
    "eav-generate-latest": "dotnet msbuild /target:GenerateManifest ./apps/__EAVFW__.__MainApp__ && dotnet test --filter \"ClassName=__EAVFW__.ArbejdstidsRegistrering.Scripts.DBDevSetup\" scripts/__EAVFW__.ArbejdstidsRegistrering.Scripts/__EAVFW__.ArbejdstidsRegistrering.Scripts.csproj",
    "blazor": "dotnet msbuild ./apps/__EAVFW__.__MainApp__/__EAVFW__.__MainApp__.csproj /target:CopyWebAssemblyToDist",
    "eav-nextjs": "dotnet new eavfw-nextjs --projectPrefix \"__EAVFW__\" --mainAppReplace \"__MainApp__\" --databaseName \"__databaseName__\""
  },
  "dependencies": {
    "next": "^12.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",

    "@fluentui/font-icons-mdl2": "8.4.2",
    "@fluentui/merge-styles": "8.5.2",
    "@fluentui/theme": "2.6.6",
    "@fluentui/utilities": "8.8.3",
    "@fluentui/react": "8.78.1",
    "@fluentui/react-hooks": "8.6.0",

    "@svgr/webpack": "^6.1.1",

    "@eavfw/manifest": "1.2.0-dev.7",
    "@eavfw/expressions": "1.0.0",
    "@eavfw/hooks": "1.0.2-dev.1",
    "@eavfw/forms": "1.0.2-dev.5",
    "@eavfw/apps": "1.1.0-dev.19",
    "@eavfw/next": "1.0.1",
    "@eavfw/utils": "1.0.1",

    "clone-deep": "4.0.1"
  },
  "devDependencies": {
    "@types/pako": "1.0.3",
    "@types/blazor__javascript-interop": "3.1.2",

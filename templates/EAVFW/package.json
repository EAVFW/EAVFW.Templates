    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "next-transpile-modules": "8.0.0",
    "prettier": "2.3.2",
    "react-fast-compare": "3.2.0",
    "typescript": "4.5.4",
    "webpack-preprocessor-loader": "1.1.4",
    "@types/clone-deep": "4.0.1"
  }
